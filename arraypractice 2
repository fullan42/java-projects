import java.util.Arrays;
import java.util.Scanner;

public class set2 {
    public static void q2(int arr[], int n) {
        boolean temp[] = new boolean[n];
        Arrays.fill(temp, false);
        // travelling in indexes for find their frequency
        for (int i = 0; i < n; i++) {
            //if the element already processed than skip it
            if (temp[i] == true)
                continue;
            // frequency value
            int value = 1;
            for (int j = i + 1; j < n; j++) {
                if (arr[i] == arr[j]) {
                    temp[j] = true;
                    value++;
                }
            }
            System.out.println("index"+" "+i+" "+arr[i] + "â€“ Frequency: " + value);
        }
    }
    public static void main(String []args) {
        Scanner sc = new Scanner(System.in);
        int i, s = 0; int t ;
        System.out.println("chose how much elements will you write");
        t= sc.nextInt();
        int dizi[] = new int[t];
        System.out.print("chose elements :");
        for (i = 0; i < dizi.length; i++) {
            dizi[i] = sc.nextInt();
        }
        int n = dizi.length;
        q2(dizi, n);
    }
}
